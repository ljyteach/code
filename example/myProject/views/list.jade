doctype html
html
  head
    title 用户列表
    body
      h1 #{result}
      table
        thead
          tr
            td 用户名
            td 密码
            td 性别
            td 年龄
        tbody
          - for(var i in data)
          tr
            - for(var j in data[i])
              td=data[i][j]
